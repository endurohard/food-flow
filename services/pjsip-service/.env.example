# Service Configuration
PORT=3009
NODE_ENV=development

# SIP Server Configuration
SIP_SERVER=sip.example.com
SIP_PORT=5060
SIP_TRANSPORT=UDP

# SIP Authentication (для каждого оператора)
SIP_USERS=operator1:password1:1001,operator2:password2:1002

# Диапазон портов для RTP (медиа)
RTP_PORT_MIN=5700
RTP_PORT_MAX=5750

# Database
DATABASE_URL=postgresql://foodflow:foodflow_secret@postgres:5432/foodflow

# Redis для кэша активных звонков
REDIS_URL=redis://redis:6379

# CORS
CORS_ORIGIN=*

# Socket.IO
SOCKETIO_PATH=/socket.io

# Мелодия входящего вызова
RINGTONE_FILE=/app/sounds/ringback.wav
